#!/usr/bin/env python

from fukei.server import FukeiSocksServer
from fukei.config import Config
from fukei import crypto


if __name__ == '__main__':
	os.path.abspath(os.path.join(os.getcwd(), os.path.pardir))
	config = Config.current()
	crypto.init_table(config.password, config.method)
	server = FukeiSocksServer(side='server', cofing=config)
	server.server_forever()